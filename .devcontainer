{
  "name": "OOPS",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "extensions": ["ms-vscode.vscode-typescript-next", "esbenp.prettier-vscode"]
    }
  },
  "postCreateCommand": "git checkout release && printenv | awk -F= '{print \"export \" $1 \"=\\\"\" $2 \"\\\"\"}' >> ~/.bashrc && source ~/.bashrc && docker-compose up --build"
}
